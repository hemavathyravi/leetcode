class Solution {
    public int lengthOfLastWord(String s) {
        // int sum=0;

        // String str= s.trim();
        // for(int i=str.length()-1;i>=0;i--)
        // {
        //     if(str.charAt(i)==' ')
        //     {
        //         break;
        //     }
        //     sum++;


        // }
        // return sum;
        String []arr=s.split(" ");
        return arr[arr.length-1].length();
        
    }
}
